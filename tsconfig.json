{
  "compilerOptions": {
    "target": "es5",
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "sourceMap": true,
    "allowJs": true,
    "allowUnreachableCode": true,
    "removeComments": true,
    "noImplicitAny": false,
    "types": [
      "es6-promise",
      "es6-collections",
      "webpack-env"
    ]
  }
}
